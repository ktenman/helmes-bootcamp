<table class="bookingDetails">
    <tr class="bookingTableHead">
        <th>Restaurant</th>
        <th>Date and time</th>
        <th>Duration</th>
        <th>Count</th>
        <th>Name</th>
        <th>Method</th>
        <th>Phone number</th>
        <th>Comments</th>
        <th>Created</th>
    </tr>
    <tr class="booking">
        <td>${booking.restaurant}</td>
        <td>${booking.date}</td>
        <td>${booking.duration}</td>
        <td>${booking.count}</td>
        <td>${booking.contactName}</td>
        <td>${booking.contactMethod}</td>
        <td>${booking.contactPhoneNumber}</td>
        <td>${booking.comments}</td>
        <td>${booking.creationTime}</td>
    </tr>
</table>